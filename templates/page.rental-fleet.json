        "heading_ED8UQG": {
          "type": "heading",
          "settings": {
            "heading": "Benefits",
            "heading_size": "h1"
          }
        },
        "text_kBfXL8": {
          "type": "text",
          "settings": {
            "text": "<ul><li>10% off our already aggressive prices for all models, with additional volume discounts for &gt; 10 bike orders (Applies to USA and Canada).<\/li><li>$300 worth of spare parts with every fleet purchased ( including sensors, tubes, lights, controllers, and more).<\/li><li>You can become part of our ambassador program through which you will be eligible to receive $50 reward every time you are referred on a new order.<\/li><\/ul>",
            "text_style": "body"
          }
        }
      },
